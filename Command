//Select a specific chipotle

SELECT * FROM yelp_db/business 
	WHERE(
	name='Chipotle' lattitude= X AND 
	longtidue= X);

//Select a range of chipotle
SELECT * FROM yelp-db/business 
	WHERE(
		name="Chipotle" AND
		lattitude BETWEEN (X, Y) AND
		longitude BETWEEN (X, Y));

//Select based on zip code
SELECT * FROM yelp-db/business 
	WHERE(
		name='Chipotle' AND
		postal_code = 78705);